{
    "name": "DOM Reader",
    "version": "1.0",
    "key": "AutoCalendarKey983023487324jkafsdjka",
    "manifest_version": 3,
    "description": "Reads the content of a page.",
    "icons": {
      "16": "icons/icon16png",
      "48": "icons/icon48png",
      "128": "icons/icon128png",
      "500": "icons/icon500.png"
    },
    "permissions": [
      "scripting",
      "activeTab",
      "identity",
      "downloads",
      "storage"
    ],
    "host_permissions": [
      "https://timetable.mypurdue.purdue.edu/*"
    ],
    "oauth2": {
      "client_id": "601689470409-27g1eot90c7pljkimmio63hss5okg9lo.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/calendar"
      ]
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/icon16png",
        "48": "icons/icon48png",
        "128": "icons/icon128png",
        "500": "icons/icon500.png"
      }
    },
    "background": {
      "service_worker": "service-worker.js"
    },
    "content_scripts": [
      {
        "matches": ["https://timetable.mypurdue.purdue.edu/Timetabling*"],
        "js": ["auto-popup.js"],
        "run_at": "document_idle"
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["icons/icon16png", "icons/icon48png", "icons/icon128png", "icons/icon500.png"],
        "matches": ["https://timetable.mypurdue.purdue.edu/*"]
      }
    ]
  }